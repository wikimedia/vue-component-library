{
	"name": "docs",
	"license": "GPL-2.0+",
	"scripts": {
		"lint": "eslint --max-warnings 0",
		"test": "npm run lint",
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview",
		"vitepress:dev": "vitepress dev docs",
		"vitepress:build": "vitepress build docs",
		"vitepress:serve": "vitepress serve docs",
		"docs:dev": "concurrently \"vue-docgen --watch\" \"vitepress dev docs\"",
		"docs:build": "vue-docgen && vitepress build docs"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^1.9.3",
		"concurrently": "^6.3.0",
		"eslint": "^7.32.0",
		"eslint-config-wikimedia": "^0.20.0",
		"typescript": "^4.4.3",
		"vite": "^2.6.5",
		"vitepress": "^0.20.0",
		"vue-docgen-cli": "^4.41.2"
	},
	"peerDependencies": {
		"vue": "^3.2.19"
	}
}
